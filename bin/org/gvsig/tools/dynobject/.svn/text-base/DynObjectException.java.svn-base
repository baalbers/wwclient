package org.gvsig.tools.dynobject;

import org.gvsig.tools.exception.BaseException;

public class DynObjectException extends BaseException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4202116374269244934L;

	public DynObjectException(Throwable cause) {
		super(
			"DynObject error.",
			cause,
			"_DynObject_error", 
			serialVersionUID
		);
	}

	protected DynObjectException(String messagefmt, String key, long code) {
		super(messagefmt, key, code);
	}

	protected DynObjectException(String messagefmt, Throwable cause, String key, long code) {
		super(messagefmt, cause, key, code);
	}
}
