package org.gvsig.sos.lib.impl.exceptions;

import org.gvsig.sos.lib.api.SOSCommException;

public class CapabilitiesRequestCreationException extends SOSCommException {

	private static String KEY = "_Capabilites_Request_Creation_Exception";
	private static String MESSAGE = "A problem occurred while performing " +
									"a capabilities request.";
	/**
	 * 
	 */
	private static final long serialVersionUID = 3314710206603646875L;

	public CapabilitiesRequestCreationException(Throwable cause) {
		super(MESSAGE, cause, KEY, serialVersionUID);
	}

}
