package org.gvsig.sos.lib.impl.exceptions;

import org.gvsig.sos.lib.api.SWEException;

public class ServerSWEException extends SWEException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2237448736766731170L;
	private static String MESSAGE = "Error occured in the SOS server.";
	private static String KEY = "_SWE_Server_Error";
	
	public ServerSWEException(Throwable cause) {
	  super(MESSAGE, cause, KEY, serialVersionUID);
	}

}
