package org.gvsig.sos.lib.impl.communication.ows.getobservations.exceptions;
/**
 * Class by operator assignation exception
 * @author Pablo Viciano Negre
 *
 */
public class OperatorNotPermitedException extends Exception{

	/**
	 * 
	 */
	private static final long serialVersionUID = 8253195957200501034L;
	
	public OperatorNotPermitedException()
	{
		super();
	}
	
	public OperatorNotPermitedException(String message)
	{
		super(message);
	}
	
	public OperatorNotPermitedException(Throwable throwable)
	{
		super(throwable);
	}
	
	public OperatorNotPermitedException(String message, Throwable throwable)
	{
		super(message,throwable);
	}
}
