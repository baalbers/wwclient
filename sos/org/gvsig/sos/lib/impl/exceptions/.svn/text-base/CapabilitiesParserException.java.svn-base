package org.gvsig.sos.lib.impl.exceptions;

import org.gvsig.sos.lib.api.SOSParserException;

public class CapabilitiesParserException extends SOSParserException {

	private static String MESSAGE = "Error parsing capabilities document";
	private static String KEY = "_Exception_parsing_Capabilities";
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -1534773611692652937L;

	public CapabilitiesParserException(Throwable cause) {
		super(MESSAGE, cause, KEY, serialVersionUID);
	}

}
